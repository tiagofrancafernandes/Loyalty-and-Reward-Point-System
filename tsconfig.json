{
    "files": [],
    "references": [
        {
            "path": "./tsconfig.node.json"
        },
        {
            "path": "./tsconfig.app.json"
        }
    ],
    "include": [
        "env.d.ts",
        "./resources/js/**/*",
        "./resources/js/**/*.js",
        "./resources/js/**/*.vue"
    ],
    "compilerOptions": {
        "composite": true,
        "noEmit": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./resources/js/*"
            ],
            "@resources/*": [
                "./resources/*"
            ],
            "@public/*": [
                "./public/*"
            ],
            "@asset/*": [
                "./public/*"
            ],
        },
    }
}
